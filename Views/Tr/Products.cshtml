
@{
    ViewBag.Title = "Products";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model BRC.Derya.WebUI.Models.UrunlerListViewModel

<section id="content">
    <div class="main">
        <div class="container">
            <h3 class="prev-indent-bot">Ürünlerimiz</h3>
            <div>
                <div>
                    @foreach (var item in Model.Urunlers)
                    {
                        <div class="p4">
                            <img src="@item.resim" style="float:right;" width="250" height="125">
                            <p class="p1"><strong class="color-2">@item.baslik</strong></p>
                            <p class="p1" style="width:600px;">
                                @item.aciklama
                            </p>

                            <a class="button-1" target="_blank" href="https://www.google.com/search?rlz=1C1CHWL_trTR816TR816&ei=Tn2XXO6rN67gkgXS4qXoDA&q=+deryabalik.com.tr+@item.baslik&oq=derya+bal%C4%B1k&gs_l=psy-ab.3..0l10.97366.99125..99421...0.0..0.239.1811.0j10j1......0....1..gws-wiz.......0i67j0i131j0i131i67.FcxallOolm4">Detaylı Arama</a>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>



<script type="text/javascript">
    $(document).ready(function () {
        $('#slider-2').bxSlider({
            pager: true,
            controls: false,
            moveSlideQty: 1,
            displaySlideQty: 4
        });
        $("a[data-gal^='prettyPhoto']").prettyPhoto({
            theme: 'facebook'
        });
    });
</script>
<script src="~/Content/cateringcom/js/jquery-1.7.1.min.js" type="text/javascript"></script>
<script src="~/Content/cateringcom/js/cufon-yui.js" type="text/javascript"></script>
<script src="~/Content/cateringcom/js/cufon-replace.js" type="text/javascript"></script>
<script src="~/Content/cateringcom/js/Dynalight_400.font.js" type="text/javascript"></script>
<script src="~/Content/cateringcom/js/FF-cash.js" type="text/javascript"></script>
<script src="~/Content/cateringcom/js/jquery.prettyPhoto.js" type="text/javascript"></script>
<script src="~/Content/cateringcom/js/hover-image.js" type="text/javascript"></script>
<script src="~/Content/cateringcom/js/jquery.easing.1.3.js" type="text/javascript"></script>
<script src="~/Content/cateringcom/js/jquery.bxSlider.js" type="text/javascript"></script>




